---
message_bus_servers:
  - host: <%= p('cf.nats.host') %>:<%= p('cf.nats.port') %>
    user: <%= p('cf.nats.username') %>
    password: <%= p('cf.nats.password') %>
external_host: <%= p('artifactory_route_name') %>.<%= p('cf.domain') %>
external_ip: <%= spec.networks.send(spec.networks.methods(false).first).ip %>
port: 8081
health_checker:
  name: standalone
  interval_in_seconds: 10
