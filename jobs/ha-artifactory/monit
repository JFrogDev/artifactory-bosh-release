check process artifactory
  with pidfile /var/vcap/sys/run/artifactory.pid
  start program "/var/vcap/jobs/ha-artifactory/bin/artifactory-ha.ctl start" with timeout 180 seconds
  stop program "/var/vcap/jobs/ha-artifactory/bin/artifactory-ha.ctl stop" with timeout 90 seconds
  if failed url http://admin:password@localhost:8081/artifactory/api/system/ping then restart
  group vcap

