check process nginx
  with pidfile /var/vcap/sys/run/nginx/nginx.pid
  start program "/var/vcap/jobs/nginx/bin/nginx_ctl start"
  stop program "/var/vcap/jobs/nginx/bin/nginx_ctl stop"
  group vcap

check process route-registrar
  with pidfile /var/vcap/sys/run/route-registrar.pid
  start program "/var/vcap/jobs/nginx/bin/route-registrar_ctl start" with timeout 10 seconds
  stop program "/var/vcap/jobs/nginx/bin/route-registrar_ctl stop" with timeout 10 seconds
  group vcap
